algoritmo "Soma"
// Função :
// Autor :
// Data : 22/08/2017
// Seção de Declarações 
var

n1,n2,s:inteiro



inicio
// Seção de Comandos
   Escreva("Informe um numero: ")
   Leia(n1)
   Escreva("Informe outro numero: ")
   Leia(n2)
   s<-n1+n2
   
   Escreval ("A media entre ",n1," e ",n2," e igual a ", s)

fimalgoritmo
